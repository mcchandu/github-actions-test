{
  "name": "citibot-qa-automation-cypress",
  "version": "1.0.0",
  "description": "It will Citibot tests with cypress framework",
  "main": "index.js",
  "scripts": {
    "test": "cypress run cypress/e2e/CitiBot/StepDefinations/*/*.cy.js",
    "test:allure": "cypress run cypress/e2e/CitiBot/StepDefinations/*/*.cy.js --browser chrome --headed --env allure=true && allure generate allure-results",
    "allure-open": "allure serve allure-results"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sourcefuse/Citibot-qa-automation-cypress.git"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^2.2.6",
    "@mmisty/cypress-allure-adapter": "^0.10.7",
    "@testing-library/cypress": "^10.0.1",
    "allure-commandline": "^2.24.0",
    "cypress": "^13.4.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.6.0",
    "cypress-xpath": "^2.0.1",
    "mocha-allure-reporter": "^1.4.0",
    "xlsx": "^0.18.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/e2e/CitiBot/StepDefinations/**"
  },
  "keywords": [
    "cypress"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sourcefuse/Citibot-qa-automation-cypress/issues"
  },
  "homepage": "https://github.com/sourcefuse/Citibot-qa-automation-cypress#readme"
}
